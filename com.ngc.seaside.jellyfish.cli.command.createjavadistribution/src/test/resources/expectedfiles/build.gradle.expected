
apply plugin: 'com.ngc.seaside.distribution'

seasideDistribution {
    buildDir = 'build'
    distributionName = "${group}.${project.name}-${version}"
    distributionDir = "build/distribution/${group}.${project.name}-${version}"
    distributionDestDir = 'build/distribution/'
}

dependencies {
   bundles project(":model.events")
   bundles project(":model.domain")
   bundles project(":model.connector")
   bundles project(":model.config")
   bundles project(":model.base")
   bundles project(":model")
}
