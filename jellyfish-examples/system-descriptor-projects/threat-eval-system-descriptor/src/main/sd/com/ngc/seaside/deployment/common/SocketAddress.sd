package com.ngc.seaside.deployment.common

data SocketAddress {
	metadata {
		"description" : "Represents a generic IPv4 or IPv6 address or hostname and port number."
	}

	string address {
		metadata {
			"description": "An IPv4 or IPv6 formatted IP address or host name."
		}
	}

	int port {
		metadata {
			"description": "A port number",
			"validation": {
				"min": 0,
				"max": 65535
			}
		}
	}
}
