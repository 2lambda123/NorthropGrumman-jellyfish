
dependencies {
   compile project(":model1.events")

   compile "com.ngc.seaside:service.api:$starfishCoreVersion"
   compile "com.ngc.seaside:service.transport.api:$transportServiceVersion"
   compile "com.ngc.blocs:service.api:$blocsCoreVersion"
   compile "org.osgi:osgi.core:$osgiVersion"
   compile "org.osgi:osgi.enterprise:$osgiVersion"
   compile "com.google.guava:guava:$guavaVersion"

   testCompile "com.ngc.blocs:test.impl.common.testutilities:$blocsCoreVersion"
   testCompile "junit:junit:$junitVersion"
   testCompile "org.mockito:mockito-core:$mockitoVersion"
}

jar {
   manifest {
      attributes("Export-Package": "com.ngc.seaside.test1.model1.*")
   }
}
