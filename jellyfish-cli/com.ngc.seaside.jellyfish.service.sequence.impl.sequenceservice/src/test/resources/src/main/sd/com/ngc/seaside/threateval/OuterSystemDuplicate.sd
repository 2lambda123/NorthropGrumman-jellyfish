package com.ngc.seaside.threateval

import com.ngc.seaside.threateval.datatype.TrackPriority
import com.ngc.seaside.classifier.datatype.Classification
import com.ngc.seaside.threateval.ClassificationTrackPriorityService

model OuterSystemDuplicate {

	input {
		Classification systemTrackClassification
	}

	output {
	   TrackPriority trackPriority
	}

	parts {
	   ClassificationTrackPriorityService classificationTrackPriorityService
	   ClassificationTrackPriorityService classificationTrackPriorityServiceDuplicate
	}

	scenario computePriorityByDelegation {
	   when receiving systemTrackClassification
	   then willPublish trackPriority
	}

	links {
	  link a systemTrackClassification -> classificationTrackPriorityService.systemTrackClassification
	  link b classificationTrackPriorityService.trackPriority -> trackPriority
	  link c systemTrackClassification -> classificationTrackPriorityServiceDuplicate.systemTrackClassification
	  link d classificationTrackPriorityServiceDuplicate.trackPriority -> trackPriority
	}
}
