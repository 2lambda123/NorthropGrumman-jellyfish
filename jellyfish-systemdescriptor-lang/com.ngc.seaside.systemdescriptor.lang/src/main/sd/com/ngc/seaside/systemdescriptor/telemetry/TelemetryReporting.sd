package com.ngc.seaside.systemdescriptor.telemetry

data TelemetryReporting {
	metadata {
		"description" : "Defines the telemetry reporting configuration for a system or service.  Data types can extend this type to enable alternative ways to configure the reporting of telemetry.  In most cases, one of these specific types of properties are applied to a model instead of applying this type as a property directly.",
		"target": "model"
	}

	int rateInMilliseconds {
		metadata {
			"description" : "Configures the rate in milliseconds that the service should report its telemetry."
		}
	}
}
