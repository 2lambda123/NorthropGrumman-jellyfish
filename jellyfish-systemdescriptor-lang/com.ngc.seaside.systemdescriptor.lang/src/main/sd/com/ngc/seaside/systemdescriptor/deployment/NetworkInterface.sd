package com.ngc.seaside.systemdescriptor.deployment

data NetworkInterface {
	metadata {
		"description" : "Represents a local network interface identified by a name.  Examples include '*', '127.0.0.0', and 'eth0'."
	}

	string name {
		metadata {
			"description": "The name of the network interface.  This name is usually OS dependent and may be a string or value similar to an IP address."
		}
	}
}
