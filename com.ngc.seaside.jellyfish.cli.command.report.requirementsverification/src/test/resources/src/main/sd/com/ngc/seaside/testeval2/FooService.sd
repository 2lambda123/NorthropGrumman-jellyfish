package com.ngc.seaside.testeval2

import com.ngc.seaside.testeval.datatype.FooStatus
import com.ngc.seaside.testeval.datatype.BarStatus

model FooService {
	metadata {
		"description": "Determines the FooNess of a system.",
		"stereotypes": ["service"],
		"satisfies": ["SW003", "SE004"]
	}
	
	input {
		FooStatus fooStatus
	}
	
	output {
		BarStatus barStatus
	}	
	
	scenario calculateFooness {
		metadata {
			"see": {"url": "http://10.207.42.43/confluence/display/SEAS/Track+Priority+Engagement+Status+Calculation" }
		}
		when receiving fooStatus
		then willPublish barStatus
		and willBeCompleted within 500 milliseconds
	}
	
}